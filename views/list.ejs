  <%- include("header") -%>
    <div class="box" id="heading">
      <h1> <%=list%> </h1>
    </div>

    <div class="box">

      <% for (i=0;i<task.length;i++){ %>
        <div class="item">
          <input type="checkbox" name="" value="">
          <p><%= task[i] %></p>
            </div>
           <%}%>


          <form  action="/" method="post">
            <input type="text" name="task" value="" placeholder="Add new task">
            <button type="submit" name="button" value="<%=list %>">+</button>
          </form>


    </div>

  <%- include("footer") -%>
